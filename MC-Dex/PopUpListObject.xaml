<Window x:Class="MC_Dex.PopUpListObject"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MC_Dex"
        mc:Ignorable="d"
        Title="PopUpListObject" Height="200" Width="400"
        WindowStyle="None"
        WindowStartupLocation="CenterScreen">

    <DockPanel>
        <TextBlock DockPanel.Dock="Top" Text="Liste des objets" FontFamily="{StaticResource Minecraft}" FontSize="22" HorizontalAlignment="Center"/>

        <Border Grid.Column="1" VerticalAlignment="Top" Height="50" Style="{StaticResource pageAjouter_borderGrid}" x:Name="ButtonValider" DockPanel.Dock="Bottom">
            <Button Background="{x:Null}" BorderThickness="0" Click="ButtonClose">
                <TextBlock Text="VALIDER" Style="{StaticResource pageAjouter_textBlock}"/>
            </Button>
        </Border>
        
        
        <ListBox>

        </ListBox>

    </DockPanel>
</Window>
